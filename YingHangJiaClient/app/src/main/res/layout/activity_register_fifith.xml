                            android:layout_width="14dp"
                            android:layout_height="14dp"
                            android:id="@+id/radioButton19" />

                        <RadioButton
                            android:layout_marginTop="4dp"
                            android:background="@drawable/text_radiobutton_background"
                            android:button="@null"
                            android:layout_width="14dp"
                            android:layout_height="14dp"
                            android:id="@+id/radioButton20" />
