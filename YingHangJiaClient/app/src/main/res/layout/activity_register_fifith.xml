                </RelativeLayout>

                <TextView
                    android:layout_marginLeft="5dp"
                    android:textSize="11dp"
                    android:textColor="#000000"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="购买这个期权"
                    android:id="@+id/textView74"
                    android:layout_alignTop="@+id/relativeLayout48"
                    android:layout_toRightOf="@+id/relativeLayout48"
                    android:layout_toEndOf="@+id/relativeLayout48" />

                <TextView
