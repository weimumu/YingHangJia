<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#eeeeee"
    android:orientation="vertical">
    <android.support.percent.PercentRelativeLayout
        android:background="#0667c8"
        android:layout_height="0dp"
        app:layout_heightPercent = "7.73%"
        android:layout_width="fill_parent">
        <RelativeLayout
            android:layout_width="60dp"
            android:layout_height="match_parent">
            <Button
                style="?android:attr/borderlessButtonStyle"
                android:background="@drawable/returnback_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/button3"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>
        <TextView
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="个人中心"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout
        app:layout_marginTopPercent="7.73%"
        android:layout_height="0dp"
        app:layout_heightPercent = "84.1%"
        android:layout_width="fill_parent">
        <RelativeLayout
            android:background="#ffffff"
            android:id="@+id/touxiang"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <TextView
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="头像"
                android:id="@+id/textView13"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <RelativeLayout
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:background="@drawable/user_photo"
                    android:layout_alignParentLeft="true"
                    android:layout_width="54dp"
                    android:layout_height="54dp"
                    android:id="@+id/user_photo_button"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:background="#ffffff"
            android:layout_below="@+id/touxiang"
            android:id="@+id/username"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <TextView
                android:textSize="20dp"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="用户名"
                android:id="@+id/textView14"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <TextView
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="888888"
                    android:id="@+id/textView20"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"/>
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:background="#ffffff"
            android:layout_below="@+id/username"
            android:id="@+id/password"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <TextView
                android:textSize="20dp"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="密码修改"
                android:id="@+id/textView15"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <RelativeLayout
                android:layout_width="45dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:background="@drawable/right_back"
                    android:layout_width="14dp"
                    android:layout_height="28dp"
                    android:id="@+id/password_change_button"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true" />
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:background="#ffffff"
            android:layout_below="@+id/password"
            android:id="@+id/personal_info"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <TextView
                android:textSize="20dp"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="个人资料"
                android:id="@+id/textView18"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <RelativeLayout
                android:layout_width="45dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:background="@drawable/right_back"
                    android:layout_width="14dp"
                    android:layout_height="28dp"
                    android:id="@+id/persenal_info_change_button"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true" />
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:background="#ffffff"
            android:layout_below="@+id/personal_info"
            android:id="@+id/test_result"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <TextView
                android:textSize="20dp"
                android:layout_marginLeft="30dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="测试结果"
                android:id="@+id/textView19"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <RelativeLayout
                android:layout_width="45dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:background="@drawable/right_back"
                    android:layout_width="14dp"
                    android:layout_height="28dp"
                    android:id="@+id/text_result_change_button"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true" />
            </RelativeLayout>
        </RelativeLayout>
    </android.support.percent.PercentRelativeLayout>
</android.support.percent.PercentRelativeLayout>