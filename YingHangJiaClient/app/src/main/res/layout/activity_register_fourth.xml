<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical">
    <android.support.percent.PercentRelativeLayout
        android:background="#0667c8"
        android:layout_height="0dp"
        app:layout_heightPercent = "7.73%"
        android:layout_width="fill_parent">
        <RelativeLayout
            android:layout_width="60dp"
            android:layout_height="match_parent">

            <Button
                style="?android:attr/borderlessButtonStyle"
                android:background="@drawable/returnback_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/back_button"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>
        <TextView
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="注册"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout
        android:background="#ffffff"
        android:layout_height="match_parent"
        app:layout_marginTopPercent = "7.73%"
        android:layout_width="fill_parent">
        <android.support.percent.PercentRelativeLayout
            android:background="#ffffff"
            app:layout_heightPercent = "8%"
            android:layout_height="0.0dp"
            android:layout_width="fill_parent">
            >
            <TextView
                android:layout_marginTop="13dp"
                android:textColor="#0667c8"
                android:textSize="18dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="风险承受能力测试"
                android:id="@+id/textView38"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />
        </android.support.percent.PercentRelativeLayout>
        <android.support.percent.PercentRelativeLayout
            android:background="#ffffff"
            app:layout_marginTopPercent = "8%"
            app:layout_heightPercent = "12%"
            android:layout_height="0.0dp"
            android:layout_width="fill_parent">
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="40dp"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/relativeLayout19">
                    <TextView
                        android:textSize="16dp"
                        android:textColor="#000000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="1、就业情况:"
                        android:id="@+id/textView28"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />

                    <Spinner
                        android:background="@drawable/name_age"
                        android:layout_marginLeft="10dp"
                        android:layout_width="140dp"
                        android:layout_height="27dp"
                        android:id="@+id/spinner"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/textView28"
                        android:layout_toEndOf="@+id/textView28" />
                </RelativeLayout>
            </RelativeLayout>
        </android.support.percent.PercentRelativeLayout>
        <android.support.percent.PercentRelativeLayout
            android:background="#ffffff"
            app:layout_marginTopPercent = "20%"
            app:layout_heightPercent = "12%"
            android:layout_height="0.0dp"
            android:layout_width="fill_parent">
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="40dp"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/relativeLayout1">
                    <TextView
                        android:textSize="16dp"
                        android:textColor="#000000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="2、家庭负担:"
                        android:id="@+id/textView1"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />

                    <Spinner
                        android:background="@drawable/name_age"
                        android:layout_marginLeft="10dp"
                        android:layout_width="140dp"
                        android:layout_height="27dp"
                        android:id="@+id/spinner1"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/textView1"
                        android:layout_toEndOf="@+id/textView1" />
                </RelativeLayout>
            </RelativeLayout>
        </android.support.percent.PercentRelativeLayout>
        <android.support.percent.PercentRelativeLayout
            android:background="#ffffff"
            app:layout_marginTopPercent = "32%"
            app:layout_heightPercent = "10%"
            android:layout_height="0.0dp"
            android:layout_width="fill_parent">
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="40dp"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/relativeLayout2">
                    <TextView
                        android:textSize="16dp"
                        android:textColor="#000000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="3、置业状况:"
                        android:id="@+id/textView2"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />

                    <Spinner
                        android:background="@drawable/name_age"
                        android:layout_marginLeft="10dp"
                        android:layout_width="140dp"
                        android:layout_height="27dp"
                        android:id="@+id/spinner2"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/textView2"
                        android:layout_toEndOf="@+id/textView2" />
                </RelativeLayout>
            </RelativeLayout>
        </android.support.percent.PercentRelativeLayout>
        <android.support.percent.PercentRelativeLayout
            android:background="#ffffff"
            app:layout_marginTopPercent = "42%"
            app:layout_heightPercent = "12%"
            android:layout_height="0.0dp"
            android:layout_width="fill_parent">
            >
            <RelativeLayout
                android:layout_centerVertical="true"
                android:layout_marginLeft="40dp"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_centerHorizontal="true"
                android:id="@+id/relativeLayout3">
                <TextView
                    android:textSize="16dp"
                    android:textColor="#000000"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="4、投资经验:"
                    android:id="@+id/textView3"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <Spinner
                    android:background="@drawable/name_age"
                    android:layout_marginLeft="10dp"
                    android:layout_width="140dp"
                    android:layout_height="27dp"
                    android:id="@+id/spinner3"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/textView3"
                    android:layout_toEndOf="@+id/textView3" />
            </RelativeLayout>
        </android.support.percent.PercentRelativeLayout>
        <android.support.percent.PercentRelativeLayout
            android:background="#ffffff"
            app:layout_marginTopPercent = "54%"
            app:layout_heightPercent = "12%"
            android:layout_height="0.0dp"
            android:layout_width="fill_parent">
            >
            <RelativeLayout
                android:layout_centerVertical="true"
                android:layout_marginLeft="40dp"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_centerHorizontal="true"
                android:id="@+id/relativeLayout4">
                <TextView
                    android:textSize="16dp"
                    android:textColor="#000000"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="5、投资知识:"
                    android:id="@+id/textView4"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <Spinner
                    android:background="@drawable/name_age"
                    android:layout_marginLeft="10dp"
                    android:layout_width="140dp"
                    android:layout_height="27dp"
                    android:id="@+id/spinner4"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/textView4"
                    android:layout_toEndOf="@+id/textView4" />
            </RelativeLayout>
        </android.support.percent.PercentRelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/relativeLayout5">
            <Button
                style="?android:attr/borderlessButtonStyle"
                android:textSize="23dp"
                android:layout_marginLeft="230dp"
                android:textColor="#ffffff"
                android:background="@drawable/next_button_background"
                android:layout_width="match_parent"
                android:layout_height="32dp"
                android:text="下一步"
                android:id="@+id/button2"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>
    </android.support.percent.PercentRelativeLayout>
</android.support.percent.PercentRelativeLayout>