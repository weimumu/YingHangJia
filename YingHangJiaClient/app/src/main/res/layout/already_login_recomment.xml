<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical">
    <android.support.percent.PercentRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_heightPercent="7.73%"
        >
        <RelativeLayout
            android:background="#0667c8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeLayout8"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <Button
                style="?android:attr/borderlessButtonStyle"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:background="@drawable/login"
                android:layout_width="32dp"
                android:layout_height="40dp"
                android:id="@+id/login_button_bar" />
            <RelativeLayout
                android:layout_alignParentRight="true"
                android:layout_width="43dp"
                android:layout_height="match_parent">
                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:layout_centerVertical="true"
                    android:background="@drawable/product"
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:id="@+id/product_button" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_centerVertical="true"
                android:background="@drawable/search_background"
                android:layout_width="180dp"
                android:layout_height="30dp"
                android:layout_centerHorizontal="true">

                <ImageView
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/search"
                    android:layout_centerVertical="true"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    />

                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:background="@null"
                    android:layout_marginLeft="29dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/search_Info_button" />
            </RelativeLayout>
        </RelativeLayout>
    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout
        app:layout_marginTopPercent="7.73%"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_heightPercent="24.54%">
        <ViewFlipper
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/photo_viewFlipper"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />
    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout
        app:layout_marginTopPercent="32.27%"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_heightPercent="2.42%"
        android:background="#eeeeee">
    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout
        app:layout_marginTopPercent="34.69%"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_heightPercent="8.25%"
        >
        <RelativeLayout
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/relativeLayout"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout11">

            <RelativeLayout
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true">

                <ImageView
                    android:background="@drawable/king_map"
                    android:layout_width="35dp"
                    android:layout_height="23dp"
                    android:id="@+id/imageView5"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true" />

                <TextView
                    android:textSize="16dp"
                    android:textColor="#254180"
                    android:layout_marginBottom="3dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="您的专属推荐"
                    android:id="@+id/textView17"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_alignParentBottom="true"
                android:layout_width="69dp"
                android:layout_height="20dp">

                <ImageView
                    android:layout_alignParentTop="true"
                    android:background="@drawable/corn"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:id="@+id/imageView8" />

                <ImageView
                    android:layout_alignParentTop="true"
                    android:background="@drawable/corn"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:id="@+id/imageView6"
                    android:layout_centerHorizontal="true" />

                <ImageView
                    android:layout_alignParentTop="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/corn"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:id="@+id/imageView7"
                    android:layout_alignParentRight="true"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_width="69dp"
                android:layout_height="20dp">
                <ImageView
                    android:layout_alignParentTop="true"
                    android:background="@drawable/corn"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:id="@+id/imageView81" />

                <ImageView
                    android:layout_alignParentTop="true"
                    android:background="@drawable/corn"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:id="@+id/imageView61"
                    android:layout_centerHorizontal="true" />

                <ImageView
                    android:layout_alignParentTop="true"
                    android:background="@drawable/corn"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:id="@+id/imageView71"
                    android:layout_alignParentRight="true"/>
            </RelativeLayout>
        </RelativeLayout>
    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout
        app:layout_marginTopPercent="42.94%"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_heightPercent="49.89%"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ListView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/product_Listview"
                android:layout_centerHorizontal="true"
                android:layout_alignParentTop="true" />
        </RelativeLayout>
    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout
        android:background="#eeeeee"
        app:layout_marginTopPercent="92.83%"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_heightPercent="7.17%"
        >
    </android.support.percent.PercentRelativeLayout>
</android.support.percent.PercentRelativeLayout>
