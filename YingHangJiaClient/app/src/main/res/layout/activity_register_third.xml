<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical">
    <android.support.percent.PercentRelativeLayout
        android:background="#0667c8"
        android:layout_height="0dp"
        app:layout_heightPercent = "7.73%"
        android:layout_width="fill_parent">
        <RelativeLayout
            android:layout_width="60dp"
            android:layout_height="match_parent">

            <Button
                style="?android:attr/borderlessButtonStyle"
                android:background="@drawable/returnback_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/button8"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>
        <TextView
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="注册"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout
        android:background="#ffffff"
        android:layout_height="match_parent"
        app:layout_marginTopPercent = "7.73%"
        android:layout_width="fill_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:id="@+id/relativeLayout6">

            <TextView
                android:textSize="18dp"
                android:textColor="#0667c8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="个人基本信息"
                android:id="@+id/textView26"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginLeft="40dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_below="@+id/relativeLayout6"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout7">

            <TextView
                android:textSize="16dp"
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1、姓名:"
                android:id="@+id/textView27"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <EditText
                android:background="@drawable/name_age"
                android:layout_marginLeft="10dp"
                android:layout_width="100dp"
                android:layout_height="25dp"
                android:id="@+id/username_edit"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/textView27"
                android:layout_toEndOf="@+id/textView27" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_marginLeft="40dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_below="@+id/relativeLayout7"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout19">
            <TextView
                android:textSize="16dp"
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="2、性别 :"
                android:id="@+id/textView28"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <Spinner
                android:background="@drawable/name_age"
                android:layout_marginLeft="10dp"
                android:layout_width="100dp"
                android:layout_height="25dp"
                android:id="@+id/sex_spinner"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/textView28"
                android:layout_toEndOf="@+id/textView28" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_marginLeft="40dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_below="@+id/relativeLayout19"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout20">
            <TextView
                android:textSize="16dp"
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="3、年龄 :"
                android:id="@+id/textView29"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <EditText
                android:background="@drawable/name_age"
                android:layout_marginLeft="10dp"
                android:layout_width="100dp"
                android:layout_height="25dp"
                android:id="@+id/age_edit"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/textView29"
                android:layout_toEndOf="@+id/textView29"
                android:digits="1234567890" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout23"
            android:layout_below="@+id/relativeLayout20">
            <TextView
                android:textSize="16dp"
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="4、手机号码:"
                android:id="@+id/textView32"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <EditText
                android:layout_marginLeft="10dp"
                android:background="@drawable/id_phone"
                android:layout_width="180dp"
                android:layout_height="25dp"
                android:id="@+id/phone_number"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/textView32"
                android:layout_toEndOf="@+id/textView32" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="5dp"
            android:layout_marginLeft="40dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_below="@+id/relativeLayout23"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout24">
            <TextView
                android:textSize="16dp"
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="5、邮箱地址:"
                android:id="@+id/textView39"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <EditText
                android:layout_marginLeft="10dp"
                android:background="@drawable/id_phone"
                android:layout_width="180dp"
                android:layout_height="25dp"
                android:id="@+id/email"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/textView39"
                android:layout_toEndOf="@+id/textView39" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/relativeLayout2">
            <Button
                style="?android:attr/borderlessButtonStyle"
                android:textSize="23dp"
                android:layout_marginLeft="230dp"
                android:textColor="#ffffff"
                android:background="@drawable/next_button_background"
                android:layout_width="match_parent"
                android:layout_height="32dp"
                android:text="下一步"
                android:id="@+id/button2"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_above="@+id/relativeLayout2"
            android:layout_centerHorizontal="true"></RelativeLayout>
    </RelativeLayout>
    </android.support.percent.PercentRelativeLayout>
</android.support.percent.PercentRelativeLayout>