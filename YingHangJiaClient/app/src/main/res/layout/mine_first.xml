            </android.support.percent.PercentRelativeLayout>
            <android.support.percent.PercentRelativeLayout
                android:layout_width="0dp"
                android:background="#eeeeee"
                android:layout_height="match_parent"
                app:layout_marginLeftPercent = "66%"
                app:layout_widthPercent = "0.8%"
                >

            </android.support.percent.PercentRelativeLayout>
            <android.support.percent.PercentRelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                app:layout_marginLeftPercent = "66.8%"
                app:layout_widthPercent = "32.6%"
                >
                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:id="@+id/collect_btn"
                    android:background="@drawable/already_collect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </android.support.percent.PercentRelativeLayout>

        </android.support.percent.PercentRelativeLayout>
        <android.support.percent.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_marginTopPercent="49%"
            app:layout_heightPercent="2%"
            android:orientation="horizontal"
            android:background="#eeeeee"
            >
        </android.support.percent.PercentRelativeLayout>
        <android.support.percent.PercentRelativeLayout
            app:layout_marginTopPercent="51%"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <android.support.percent.PercentRelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                app:layout_widthPercent = "32.6%"
                >
                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:id="@+id/news_collect_btn"
                    android:background="@drawable/zixun_collect"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </android.support.percent.PercentRelativeLayout>
            <android.support.percent.PercentRelativeLayout
                android:background="#eeeeee"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                app:layout_marginLeftPercent = "32.6%"
                app:layout_widthPercent = "0.8%"
                >
            </android.support.percent.PercentRelativeLayout>
            <android.support.percent.PercentRelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                app:layout_marginLeftPercent = "33.4%"
                app:layout_widthPercent = "32.6%"
                >
                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:id="@+id/score_btn"
                    android:background="@drawable/jifen"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </android.support.percent.PercentRelativeLayout>
            <android.support.percent.PercentRelativeLayout
                android:layout_width="0dp"
                android:background="#eeeeee"
                android:layout_height="match_parent"
                app:layout_marginLeftPercent = "66%"
                app:layout_widthPercent = "0.8%"
                >
            </android.support.percent.PercentRelativeLayout>
            <android.support.percent.PercentRelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                app:layout_marginLeftPercent = "66.8%"
                app:layout_widthPercent = "32.6%"
                >
                <Button
                    style="?android:attr/borderlessButtonStyle"
                    android:id="@+id/coustomer_server_btn"
                    android:background="@drawable/licaiguwen"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </android.support.percent.PercentRelativeLayout>

        </android.support.percent.PercentRelativeLayout>
        </android.support.percent.PercentRelativeLayout>

    <android.support.percent.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_marginTopPercent="91.87%"
        android:background="#eeeeee"
            >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                style="?android:attr/borderlessButtonStyle"
                android:textSize="18dp"
                android:background="@drawable/log_out_button_background"
                android:textColor="#ffffff"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:text="退出登录"
                android:id="@+id/button"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>
        </android.support.percent.PercentRelativeLayout>

    </android.support.percent.PercentRelativeLayout>

    </android.support.percent.PercentRelativeLayout>