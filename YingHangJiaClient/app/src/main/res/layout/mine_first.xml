<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical">

    <android.support.percent.PercentRelativeLayout
        app:layout_marginLeftPercent="52%"
       android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_widthPercent="49%"
        android:layout_gravity="right">
    </android.support.percent.PercentRelativeLayout>

    <android.support.percent.PercentRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_heightPercent="100%">
        <android.support.percent.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_heightPercent="7.73%"
            android:background="#0667c8"
            >
            <TextView
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="我的"
                android:textSize="20dp"
                android:textColor="#ffffff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </android.support.percent.PercentRelativeLayout>

        <android.support.percent.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_marginTopPercent="7.73%"
            app:layout_heightPercent="49.52%"
            android:orientation="vertical"
            >
            <android.support.percent.PercentRelativeLayout
                android:background="@drawable/me_top_background"
                app:layout_heightPercent="78.21%"
                android:layout_width="match_parent"
                android:layout_height="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        style="?android:attr/borderlessButtonStyle"
                        android:background="@drawable/user_phone"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:id="@+id/button6"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:background="@drawable/user_name_textview_background"
                        android:textSize="15dp"
                        android:textColor="#ffffff"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="用户名: xxxxxx"
                        android:id="@+id/textView8"
                        android:layout_below="@+id/button6"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="18dp" />
                </RelativeLayout>
            </android.support.percent.PercentRelativeLayout>
            <android.support.percent.PercentRelativeLayout
                app:layout_marginTopPercent="78.21%"
                android:background="#1561af"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
                <android.support.percent.PercentRelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    app:layout_widthPercent="49%">
                    <RelativeLayout
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:layout_width="160dp"
                        android:layout_height="35dp">

                        <TextView
                            android:textSize="17dp"
                            android:textColor="#ffffff"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="预期收益"
                            android:id="@+id/textView5"
                            android:layout_alignParentLeft="true" />


                        <RelativeLayout
                            android:background="@drawable/yugushouyi_background"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_width="87dp"
                            android:layout_height="30dp"
                            android:layout_alignParentRight="true"
                                                    >

                            <TextView
                                android:textSize="15dp"
                                android:textColor="#ffde2b"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="10000RMB"
                                android:id="@+id/textView6" />
                        </RelativeLayout>
                    </RelativeLayout>
                </android.support.percent.PercentRelativeLayout>
                <ImageView
                    android:background="@drawable/yugu_xuxian"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:layout_width="2dp"
                    android:layout_height="match_parent" />
                <android.support.percent.PercentRelativeLayout
                    android:layout_width="0dp"
                    app:layout_marginLeftPercent="52%"
                    android:layout_height="match_parent"
                    app:layout_widthPercent="49%"
                    >
                    <RelativeLayout
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:layout_width="160dp"
                        android:layout_height="35dp">

                        <TextView
                            android:textSize="17dp"
                            android:textColor="#ffffff"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="预期资产"
                            android:id="@+id/textView54"
                            android:layout_alignParentLeft="true" />


                        <RelativeLayout
                            android:background="@drawable/yugushouyi_background"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_width="87dp"
                            android:layout_height="30dp"
                            android:layout_alignParentRight="true"
                            >

                            <TextView
                                android:textSize="15dp"
                                android:textColor="#ffde2b"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="10000RMB"
                                android:id="@+id/textView7" />
                        </RelativeLayout>
                    </RelativeLayout>
                </android.support.percent.PercentRelativeLayout>

            </android.support.percent.PercentRelativeLayout>

            </android.support.percent.PercentRelativeLayout>


    <android.support.percent.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_marginTopPercent="57.25%"
            app:layout_heightPercent="2.62%"
            android:background="#eeeeee"
            >
        </android.support.percent.PercentRelativeLayout>

    <android.support.percent.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_marginTopPercent="59.87%"
            app:layout_heightPercent="32%"
            android:orientation="vertical"
            >
        <android.support.percent.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_heightPercent="49%"
            android:orientation="horizontal"
            >
            <android.support.percent.PercentRelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                app:layout_widthPercent = "32.6%"
                >
            <Button
